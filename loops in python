# Iterating over a list
my_list = [1, 2, 3, 4, 5]
for item in my_list:
    print(item)
# Output: 1 2 3 4 5

# Iterating over a range
for i in range(5):
    print(i)
# Output: 0 1 2 3 4

# Iterating over a string
my_string = "Hello"
for char in my_string:
    print(char)
# Output: H e l l o

# Iterating over a tuple
my_tuple = (10, 20, 30)
for item in my_tuple:
    print(item)
# Output: 10 20 30



# Else with for loop
for i in range(5):
    print(i)
else:
    print("Loop completed successfully")
# Output: 0 1 2 3 4 Loop completed successfully

# Else with while loop
count = 0
while count < 3:
    print(count)
    count += 1
else:
    print("While loop completed")
# Output: 0 1 2 While loop completed# Else with for loop
for i in range(5):
    print(i)
else:
    print("Loop completed successfully")
# Output: 0 1 2 3 4 Loop completed successfully


    count += 1
# Output: 0 1 2 3 4






# Break statement
for i in range(10):
    if i == 5:
        break
    print(i)
# Output: 0 1 2 3 4

# Continue statement
for i in range(10):
    if i % 2 == 0:
        continue
    print(i)
# Output: 1 3 5 7 9

# Pass statement
for i in range(5):
    if i == 2:
        pass # Placeholder, no action is taken
    print(i)
# Output: 0 1 2 3 4







